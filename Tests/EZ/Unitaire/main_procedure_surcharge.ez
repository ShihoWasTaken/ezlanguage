program TestProcedure


procedure addition (variable A, B, C are integer)
    C = A + B
end procedure

procedure addition (variable A, B, D, C are integer)
    C = A + B + D
end procedure


procedure TestProcedure ()
    variable a, b, c, d are integer
    a = 5
    b = 10
    d = 10
    
    addition(a, b, c)
